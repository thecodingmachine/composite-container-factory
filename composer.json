{
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:thecodingmachine/container-discovery.git"
        }
    ],
    "name": "thecodingmachine/composite-container-factory",
    "type": "library",
    "description": "This package provides a factory creating a composite container that automatically detects compatible containers using container-discovery and Puli.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TheCodingMachine\\CompositeContainer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheCodingMachine\\CompositeContainer\\": "tests/"
        }
    },
    "require": {
        "acclimate/container": "~1.0",
        "thecodingmachine/container-discovery": "dev-master",
        "mouf/picotainer": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "puli/cli": "~1.0",
        "puli/composer-plugin": "~1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
